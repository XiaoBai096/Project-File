import turtle as t

t.hideturtle()
t.color("red")
t.circle(120)
t.color("black","yellow")

angle = 60

for i in range(12):
    angle += 30
    t.up()
    t.goto(0,120)
    t.seth(angle)
    t.fd(120)
    t.down()

    t.left(30)
    t.begin_fill()
    t.fd(80)
    t.right(60)
    t.fd(80)
    t.right(120)
    t.fd(80)
    t.right(60)
    t.fd(80)
    t.end_fill()
